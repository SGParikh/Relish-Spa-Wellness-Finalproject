<div class="Subheading">
    <!-- Section heading -->
    <h2 class="h2-responsive font-weight-bold text-left my-3">{{heading}}</h2>
</div>
<div class="accordion" id="accordionExample">
        <div class="card">
        <div class="card-header" id="headingOne" data-toggle="collapse" data-target="#collapseOne" aria-expanded="true"
            aria-controls="collapseOne">
            <h4 class="h4-responsive font-weight-bold text-center my-3">Massages</h4>
        </div>
        <div id="collapseOne" class="collapse show" aria-labelledby="headingOne" data-parent="#accordionExample">

        <sectipn class="Massages">

        <div class="card-deck services">
{{# each services}}

                    <div class="card">
                        <img src="{{this.imagePath}}" class="card-img-top" alt="{{this.title}}" style="height: 150px;">
                        <div class="card-body">
                            <h4 class="card-title">
                                <h4>{{this.title}}</h4>
                            </h4>
                            <p class="card-text">{{this.description}}</p>
                            <p class="card-text">60 minutes : <strong>${{this.price60}}</strong><br> 90 minutes :
                                <strong>${{this.price90}}</strong></p>
                        </div>
                        <div class="card-footer">
                            <a href="#" data-toggle="modal" data-target="#{{this.id}}Massage">Read More</a>
                        </div>
                    </div> 
      
{{/each}}

                </div>
            </sectipn>
        </div>
    </div>

    <div class="card">
            <div class="card-header" id="headingTwo" data-toggle="collapse" data-target="#collapseTwo" aria-expanded="false"
                aria-controls="collapseTwo">
