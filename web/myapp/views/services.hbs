                            <p class="card-text">{{this.description}}</p>
                            <p class="card-text">60 minutes : <strong>{{this.price60}}</strong></p>
                        </div>
                         <div class="card-footer">
                            <a href="#" data-toggle="modal" data-target="#{{this.id}}Bodycare">Read More</a>
                        </div>
                    </div>
    {{/each}}

                    </div>
                </section>
            </div>
    </div>
